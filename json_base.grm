number ::= <int2>:i               => i

int ::= '-'<num>:i               => i * -1
      | <num>:i                  => i

int2 ::= <digit>+:ds             => reduce(lambda x,y : x * 10 + y, ds)
       | -<digits>+:s            => -1 * reduce(lambda x,y : x * 10 + y, ds)

num ::= <num>:n <digit>:d        => n * 10 + d
      | <digit>:d                => d
